# EditorConfig is awesome: https://EditorConfig.org

# Root configuration file
root = true

# All files
[*]
charset = utf-8
end_of_line = lf
indent_style = space
indent_size = 4
tab_width = 4
trim_trailing_whitespace = true
insert_final_newline = true

# Java files
[*.java]
indent_style = space
indent_size = 4
tab_width = 4
